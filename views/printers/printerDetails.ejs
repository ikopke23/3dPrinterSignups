<%if(user == null){%>
    <%- include('../partials/header', {title:"Design Lab 2.0"})-%>
<%}else {%>
    <%- include('../partials/userHeader', {title:"Design Lab 2.0"})-%>
<%}%>

<h1 class="fs-1"><%=printer["name"]%></h1>
<div>
    <img class="rounded-lg-3 float-start" src="../images/<%=printer["photo"]%>" width="500" height="500">
    <img class="rounded-lg-3 float-end " src="../images/<%=printer["location"]%>" width="600" height="500">
</div>

<span class="hidden"></span>

<table class="table table-striped table-hover table-primary">
    <tbody>
        <tr>
            <td>Status</td>
            <td><%=printer["status"]%></td>
        </tr>
        <tr>
            <td>Last Service</td>
            <td><%=printer["lastService"]%></td>
        </tr>
        <tr>
            <td>Printing Time</td>
            <td><%=printer["printingTime"]%></td>
        </tr>

        <!-- <tr>
            <td></td>
            <td></td>
        </tr> -->

    </tbody>
</table>
<span class="hidden"></span>
<table class="table table-primary">
    <thead>
        <tr>
            <td> errors</td>

        </tr>
    </thead>
    <tbody>
        <%for(let i = 0; i < printer["errors"].length; i++){%>
            <tr>
                <td> <%=printer["errors"][i]%></td>
            </tr>
        <%}%>
    </tbody>
</table>

<table class="table table-primary">
    <thead>
        <tr>
            <td> Prints</td>
        </tr>
    </thead>
    <tbody>
        <%for(let i = 0; i < printer["prints"].length; i++){%>
            <tr>
                <td> <%=printer["prints"][i]%></td>
            </tr>
        <%}%>
    </tbody>
</table>


<%- include('../partials/footer') -%>
