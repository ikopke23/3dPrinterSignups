<%- include('partials/header', {title:"print[name]"}) -%>



<!-- This is where I put all the specifics about each print -->
<div>
  <img class="rounded-lg-3" src="images/<%=print["photo"]%>">
</div>

<div class="hidden">  </div>

<!-- console.log(print) -->
<table class="table table-striped table-primary table-hover">
  <thead>
      <td colspan="2"><%=print["name"]%></td>
  </thead>
  <tbody>
    <tr>
      <td>Description:</td>
      <td><%=print["description"]%></td>
    </tr>
    <tr>
      <td>Printer:</td>
      <td><%=print["printer"]%></td>
    </tr>
    <tr>
      <td>Start Time</td>
      <script>
        let data = print["date"];
        let dataA = data.split("/")
        let st = dataA[3];
        console.log("st = "+ st);
      </script>
      <td><%=st%></td>
    </tr>
    <tr>
      <td>Estimated time at 100%</td>
      <td><%=Math.floor(print["time"]/60)%> Hours <%=print["time"]%60%> minutes </td>
    </tr>

    <tr>
        <<td>Infill</td>
        <td><%=print["infill"]%>%</td>
    </tr>
    <tr>
      <td>Filament Width</td>
      <td><%=print["width"]%></td>
    </tr>
    <tr>
      <td colspan="2"><a href="<%=print["link"]%>" target="_blank">Link</a></td>
    </tr>
  </tbody>
</table>
